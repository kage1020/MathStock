{
  "cSpell.words": ["astrojs", "rehype"],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "astro"
  ],
  "editor.formatOnSave": true,
  "prettier.documentSelectors": ["**/*.astro", "**/*.mdx"],
  "prettier.configPath": "./.prettierrc.json",
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
